<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dekker_app.dekker_app" version="1.2.17" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>DEKKERAPP</name>
    <description>
        DEKKERAPP
    </description>
    <author email="mlopez@sfd.com.mx" href="http://sanfranciscodekkerlab.com.mx">
        San Francisco Dekkerlab
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="http://dkmatrizz.ddns.net/* " />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-targetSdkVersion" value="29" />
    <preference name="android-minSdkVersion" value="22" />
    <preference name="android-maxSdkVersion" value="29" />
    <preference name="orientation" value="portrait" />
</widget>
